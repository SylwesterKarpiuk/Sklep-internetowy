@model IEnumerable<Shop___videopoint.Models.Review>

<ul>
    @foreach (var item in Model)
    {
        <li>
            <div>
                <h4>@item.Rating / 10</h4>
                <small>Author: @item.Name</small>
            </div>
            <p>@Html.DisplayFor(modelItem => item.Comment)</p>
            <p><small>@Html.ActionLink("Edit","Edit", new { id = item.id })</small></p>
        </li>
    }
</ul>





